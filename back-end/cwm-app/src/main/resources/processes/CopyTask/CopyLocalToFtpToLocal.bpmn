  <process id="copyLocalToFtoToLocalProcess" name="Copy Local To FTP To Local Process" isExecutable="true">
    <documentation>Uploads a file from local repository to FTP repository. Afterwards, it downloads the FTP copy to a local repository</documentation>
    <startEvent id="startevent1" name="Start"></startEvent>
    <serviceTask id="copytask1" name="Copy Task (Local - FTP) " activiti:delegateExpression="${copyTaskBehavior}">
      <extensionElements>
        <activiti:field name="destinationFileHandler">
